{
  "name": "guestcheck.io-landingpage",
  "version": "1.0.0",
  "description": "corona registration service",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "prettier": "prettier --write src/**/*.{scss,html}",
    "htmlhint": "htmlhint src/**/*.html",
    "stylelint": "stylelint src/scss",
    "stylelint:fix": "npm run stylelint -- --fix",
    "eslint": "eslint src/js/**/*.js",
    "eslint:fix": "npm run eslint -- --fix",
    "fix": "npm run eslint:fix && npm run prettier && npm run stylelint:fix",
    "lint": "npm run eslint && npm run htmlhint && npm run stylelint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run eslint:fix"
    ],
    "src/**/*.scss": [
      "npm run stylelint:fix"
    ],
    "src/**/*.html": [
      "npm run htmlhint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "guestcheck.io-landingpage"
  },
  "keywords": [
    "corona-regisration",
    "landingpage",
    "landing-page-corona",
    "corona-data-service"
  ],
  "author": "uwe jettkant",
  "license": "ISC",
  "devDependencies": {
    "@namics/stylelint-bem": "^6.3.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "htmlhint": "^0.14.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "sass": "^1.26.11",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0"
  }
}
